{
  "title": "EchoMind Overview",
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Audio Frames",
      "targets": [
        { "expr": "sum(echomind_meeting_frames_total)" }
      ]
    },
    {
      "type": "stat",
      "title": "STT Segments",
      "targets": [
